<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>schedule-2</title>
    <link rel="stylesheet" type="text/css" href="/resources/lib/bootstrap/bootstrap.css"/>
    <link rel="stylesheet" type="text/css" href="/resources/css/match/style.1.0.css"/>

    <script src="/resources/js/match/respond.js"></script>
    <script src="/resources/lib/jquery/jquery-3.1.1.min.js"></script>
    <script type="text/javascript" src="/resources/lib/bootstrap/bootstrap.min.js"></script>
    <script type="text/javascript" src="/resources/js/match/matchDetailsAdmin.js"></script>

</head>
<body>
<nav class="nav-bg navbar">
    <div class="container">
        <div class="logo-w col-lg-4 col-md-5 col-sm-11 col-xs-11">
            <img src="/resources/img/logo足球项目组.png"/>
            <span><a href="index.jsp">四川省青少年校园足球信息网</a></span>
        </div>
        <div class="nav-b hidden-lg hidden-md col-sm-1 hidden-xs">
            <button type="button" class="glyphicon glyphicon-th-list"></button>
        </div>
        <div class="nav-w col-lg-8 col-md-7 hidden-sm hidden-xs">
            <ul>
                <li><a href="matchList.jsp">赛事赛程</a></li>
                <li><a href="#">教练管理</a></li>
                <li><a href="#">球员管理</a></li>
                <li><a href="#">球队管理</a></li>
                <li><a href="#">个人中心</a></li>
            </ul>
        </div>
    </div>
</nav>
<div class="container ml-h" id="titleName">
</div>
<input hidden id="mid" value="${mid}"/>
<div class="container ml-tab">
    <div class="bs-example bs-example-tabs" data-example-id="togglable-tabs">
        <ul class="nav nav-tabs" id="myTabs" role="tablist">
            <li class="active" role="presentation"><a id="home-tab" role="tab" aria-expanded="true" aria-controls="home"
                                                      href="#home" data-toggle="tab">详情</a></li>
            <li role="presentation" id="schedule-list"><a id="profile-tab" role="tab" aria-expanded="false"
                                                          aria-controls="profile" href="#profile"
                                                          data-toggle="tab">赛程表</a></li>
            <li class="dropdown" role="presentation"><a id="dropdown1-tab" role="tab" aria-controls="dropdown1"
                                                        href="#dropdown1" data-toggle="tab">审核表</a></li>
        </ul>
        <!--标签页-->
        <div class="tab-content" id="myTabContent">
            <!--第一个标签-->
            <div class="tab-pane fade active in sch-w" id="home" role="tabpanel" aria-labelledby="home-tab">
                <div>赛事名称: <b><span id="matchName"></span></b></div>
                <div>赛事级别：<b><span id="matchLevel"></span></b></div>
                <div>赛事状态：<b><span id="matchSaute"></span></b></div>
                <div>赛制：<b><span id="matchNumPlayer"></span></b></div>
                <div>赛事开始时间：<b><span id="matchStartTime"></span></b></div>
                <div>报名截止时间：<b><span id="matchDeadLine"></span></b></div>
            </div>
            <!--第二个标签-->
            <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                <a href="addSch.jsp" style="color: #fff;">
                    <button class="btn btn-primary" style="float: right;margin-right: 10px;margin-top: 3.5px;">新增
                    </button>
                </a>
                <div class="tab-4" id="schedule-data">
                    <table class="table table-hover">
                        <thead>
                        <tr>
                            <th>开赛时间</th>
                            <th>地点</th>
                            <th>主队</th>
                            <th>得分</th>
                            <th>客队</th>
                            <th>状态</th>
                            <th>操作</th>
                        </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
            <!--第三个标签-->
            <div class="tab-pane fade" id="dropdown1" role="tabpanel" aria-labelledby="dropdown1-tab">
                <div class="tab-3">
                    <table class="table table-hover">
                        <thead>
                        <tr>
                            <th>审核参赛机构名称</th>
                            <th>参赛队伍</th>
                            <th>审核操作</th>
                        </tr>
                        </thead>
                        <tbody id="body">

                        </tbody>
                    </table>
                </div>
            </div>
            <div class="pag" id="pageInfo">

            </div>

        </div>
    </div>
</div>

</body>
</html>
