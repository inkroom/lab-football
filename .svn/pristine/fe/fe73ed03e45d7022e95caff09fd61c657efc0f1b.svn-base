.btn3 {
 color: #fff;
 background-color: #5cb85c;
 border-color: #4cae4c;
 outline: none;
 display: inline-block;
 padding: 6px 12px;
 margin-bottom: 0;
 font-size: 14px;
 font-weight: 400;
 line-height: 1.42857143;
 text-align: center;
 white-space: nowrap;
 vertical-align: middle;
 -ms-touch-action: manipulation;
 touch-action: manipulation;
 cursor: pointer;
 -webkit-user-select: none;
 -moz-user-select: none;
 -ms-user-select: none;
 user-select: none;
 background-image: none;
 border: 1px solid transparent;
 border-radius: 4px;
}
.btn3-default {
 color: #333;
 background-color: #fff;
 border-color: #ccc;
}
.btn3-default:hover {
 color: #333;
 background-color: #e6e6e6;
 border-color: #adadad;
}
.btn3-success:hover {
 color: #fff;
 background-color: #449d44;
 border-color: #398439;
 text-decoration: none;
}
.btn3-info {
 color: #fff;
 background-color: #5bc0de;
 border-color: #46b8da;
}
.btn3-info:hover {
 color: #fff;
 background-color: #31b0d5;
 border-color: #269abc;
}
.btn3-warning {
 color: #fff;
 background-color: #f0ad4e;
 border-color: #eea236;
}

.btn3-warning:hover {
 color: #fff;
 background-color: #ec971f;
 border-color: #d58512;
}

.btn3-danger {
 color: #fff;
 background-color: #d9534f;
 border-color: #d43f3a;
}

.btn3-danger:hover {
 color: #fff;
 background-color: #c9302c;
 border-color: #ac2925;
}
.btn3-large{
 height:40px;
}
.btn3-primary {
 color: #fff;
 background-color: #337ab7;
 border-color: #2e6da4;
}
.btn3-primary:hover {
 color: #fff;
 background-color: #286090;
 border-color: #204d74;
}