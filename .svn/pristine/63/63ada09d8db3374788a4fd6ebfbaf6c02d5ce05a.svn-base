<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>add-match</title>
    <link rel="stylesheet" type="text/css" href="/resources/css/bootstrap.min.css"/>
    <link rel="stylesheet" type="text/css" href="/resources/css/match/style.1.0.css"/>

    <script src="/resources/js/match/respond.js"></script>
    <script src="/resources/lib/jquery/jquery-1.11.3.js"></script>
    <script type="text/javascript" src="/resources/js/match/bootstrap2.min.js"></script>

</head>
<body>
<nav class="nav-bg navbar">
    <div class="container">
        <div class="logo-w col-lg-4 col-md-5 col-sm-11 col-xs-11">
            <img src="/resources/img/match/logo足球项目组.png"/>
            <span><a href="index.jsp">四川省青少年校园足球信息网</a></span>
        </div>
        <div class="nav-b hidden-lg hidden-md col-sm-1 hidden-xs">
            <button type="button" class="glyphicon glyphicon-th-list"></button>
        </div>
        <div class="nav-w col-lg-8 col-md-7 hidden-sm hidden-xs">
            <ul>
                <li><a href="matchList.jsp">赛事赛程</a></li>
                <li><a href="#">教练管理</a></li>
                <li><a href="#">球员管理</a></li>
                <li><a href="#">球队管理</a></li>
                <li><a href="#">个人中心</a></li>
            </ul>
        </div>
    </div>
</nav>
<div class="container mo-h">
    新增赛程
</div>
<div class="container">
    <form class="form-horizontal">
        <div class="form-group">
            <label class="col-sm-2 control-label">主队</label>
            <div class="col-sm-10">
                <select class="form-control input-sm">
                    <option>选着主队的队伍</option>
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                    <option>6</option>
                    <option>7</option>
                    <option>8</option>
                    <option>9</option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">客队</label>
            <div class="col-sm-10">
                <select class="form-control input-sm">
                    <option>选着客队的队伍</option>
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                    <option>6</option>
                    <option>7</option>
                    <option>8</option>
                    <option>9</option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">开赛时间</label>
            <div class="col-sm-10">
                <input type="email" class="form-control" placeholder="请填写比赛时间">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">开赛地点</label>
            <div class="col-sm-10">
                <input type="email" class="form-control" placeholder="请填写比赛地点">
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <a href="matchDetailsAdmin.jsp">
                    <button type="submit" class="btn btn-default">确认</button>
                </a>
                <a href="matchDetailsAdmin.jsp">
                    <button type="submit" class="btn btn-default">取消</button>
                </a>
            </div>
        </div>
    </form>
</div>
</body>
</html>

