<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="cn.nsu.edu.web.four.daos.jdbc.team.TeamInfosToTeamMapper">

    <insert id="insertTeamInfo" parameterType="cn.nsu.edu.web.four.beans.teams.TeamInfos" useGeneratedKeys="true" keyProperty="teamId">
        INSERT INTO Team(name,type,status,create_time,org_id)
        VALUES(#{teamName},#{teamClazz},#{teamStatus},NOW(),#{orgId})
    </insert>

    <select id="selectTeamInfo" resultType="Integer">
        SELECT id_team
        FROM Team
        WHERE name=#{teamName}
    </select>

</mapper>