<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.nsu.edu.web.four.daos.jdbc.schedule.MechanismMapper">
    <resultMap id="MechanismMap" type="cn.nsu.edu.web.four.beans.schedule.Memchanism">
        <id column="id_org" jdbcType="INTEGER" property="idOrg" />
        <result column="name" jdbcType="VARCHAR" property="name" />
    </resultMap>

<select id="getMechanismList" resultMap="MechanismMap">
    SELECT id_org,name
    FROM Organization
</select>
</mapper>