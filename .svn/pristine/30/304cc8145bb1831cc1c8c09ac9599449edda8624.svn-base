package com.nsu.bean.teacher;

/**
 * @author ChenGang
 * @Title: QuesOfNumBean
 * @Package com.nsu.bean.teacher.information
 * @Description:题的数量的bean，用来转换json
 * @date 2017/8/10 0010 上午 11:46
 **/
public class QuesOfNumBean{
    public int getSumnum() {
        return sumnum;
    }

    public void setSumnum(int sumnum) {
        this.sumnum = sumnum;
    }

    private int sumnum;//题的总量
    private int easynum;//容题数量
    private int hardnum;//难题数量

    public int getHardnum() {
        return hardnum;
    }

    public void setHardnum(int hardnum) {
        this.hardnum = hardnum;
    }

    public int getEasynum() {
        return easynum;

    }

    public void setEasynum(int easynum) {
        this.easynum = easynum;
    }

    @Override
    public String toString() {
        return "QuesOfNumBean{" +
                "sumnum=" + sumnum +
                ", easynum=" + easynum +
                ", hardnum=" + hardnum +
                '}';
    }
}
