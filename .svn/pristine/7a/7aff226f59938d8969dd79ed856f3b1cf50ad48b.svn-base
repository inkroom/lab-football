package com.nsu.bean.student.profile;

import java.util.Date;
import java.util.List;
import java.util.Map;


public class ProfileBean {
    private int StudentId;
    private String StudentName;
    private String Sex;
    private String IdNumber;
    private String NowSchoolName;
    private String NowClassName;
    private List<String> PastSchool;
    private List<String> QualificationsPath;

    public ProfileBean() {
    }


    public List<String> getPastSchool() {
        return PastSchool;
    }

    public void setPastSchool(List<String> pastSchool) {
        PastSchool = pastSchool;
    }
    public void setStudentId(int studentId) {
        StudentId = studentId;
    }

    public void setStudentName(String studentName) {
        StudentName = studentName;
    }

    public void setSex(String sex) {
        Sex = sex;
    }

    public void setIdNumber(String idNumber) {
        IdNumber = idNumber;
    }


    public void setQualificationsPath(List<String> qualificationsPath) {
        QualificationsPath = qualificationsPath;
    }

    public int getStudentId() {
        return StudentId;
    }

    public String getStudentName() {
        return StudentName;
    }

    public String getIdNumber() {
        return IdNumber;
    }


    public List<String> getQualificationsPath() {
        return QualificationsPath;
    }

    public String getSex() {
        return Sex;
    }

    public String getNowSchoolName() {
        return NowSchoolName;
    }

    public void setNowSchoolName(String nowSchoolName) {
        NowSchoolName = nowSchoolName;
    }

    public String getNowClassName() {
        return NowClassName;
    }

    public void setNowClassName(String nowClassName) {
        NowClassName = nowClassName;
    }
}
