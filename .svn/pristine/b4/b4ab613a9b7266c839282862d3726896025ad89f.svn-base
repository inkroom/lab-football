        <script type="text/javascript">

            lastSelect = document.getElementById("t0")  //初始化
            function changeTab(cur_this)
            {
                if (cur_this !== lastSelect)
                {
                    $(lastSelect).attr("class", "");   //select失去焦点
                    $("#" + $(lastSelect).attr("tab")).attr("class", "");    //隐藏tab
                    lastSelect = cur_this;
                    $(cur_this).attr("class", "selected");    //select获取焦点
                    $("#" + $(cur_this).attr("tab")).attr("class", "show");   //显示tab
                }
            }

