.cs-bg{
    background-image: url(../../../resources/img/player/SoccerImage.jpg);
    background-size:100% 100%;
    position:fixed;
    /*overflow:auto;!*页面上下滚动*!*/
    top:0;
    left:0;
    width:100%;
    height:100%;
}

.container{
    display:table;
    height:100%;
}

.row {
    display: table-cell;
    vertical-align: middle;
}

.box_OI{
    width: 460px;
    height: 440px;
    margin: 0 auto;
    padding: 30px 32px;
    display: flex;
}

.login_bg{
    background: rgba(255,255,255,1);
    border-radius: 5px;
    align-self: center;
}

.logo-w2{
    padding: 0;
    float: left;
}

.title{

}

/*!*取消所有外边框*!*/
input,button,select,textarea{
    outline:none;
}

/*每一行间距*/
.put2{
    margin-bottom: 25px;
}

/*输入框*/
.input{
    border-radius: 4px;
    border: none;
    height: 40px;
    width: 100%;
    text-indent: 10px;
    background: #eeeeee;
    opacity: 0.6;
}
.input2{
    text-indent: 10px;
    border-radius: 4px;
    opacity: 0.8;
    /*border: 1px solid gray;*/
    background: #eeeeee;
    border: none;
    width: 49.5%;
    height: 40px;
    float: left;
}

/*输入框提示字体*/
input::-webkit-input-placeholder { /* WebKit browsers */
    font-family: 微软雅黑;
    color:#777777;
    font-size: 13px;
}
input:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    font-family: 微软雅黑;
    color:#777777;
    font-size: 13px;
}
input::-moz-placeholder { /* Mozilla Firefox 19+ */
    font-family: 微软雅黑;
    color:#777777;
    font-size: 13px;
}
input:-ms-input-placeholder { /* Internet Explorer 10+ */
    font-family: 微软雅黑;
    color:#777777;
    font-size: 13px;
}

/*机构登录按钮*/
.btn1{
    border: none;
    width: 180px;
    height: 40px;
    font-size: 18px;
    /*font-weight:bold;*/
    /*opacity: 0.7;*/
    border-radius: 5px;
    margin-top: 10px;
    margin-bottom: 20px;
}

/*-清除IE下的earch叉号*/
::-ms-clear,::-ms-reveal{
    display: none;
}

