* {
    border: none;
    margin: 0;
    padding: 0;
    list-style: none;
}
html,
body {
    width: 100%;
    height: 100%;
}
.wrapper {
    width: 900px;
    left:50%;
    margin-left:-450px;
    position:absolute;
}
.wrapper .top_ {
    width: 100%;
    height: 180px;
}
.wrapper .top_ h2 {
    float: left;
    line-height: 100px;
    font-size: 30px;

}

.wrapper .top_ a {
    text-decoration: none;
    color: #ccc;
    display: inline-block;
    padding: 6px 25px;
    margin-top: 40px;
    float: right;
    margin-right: 20px;
    border: 1px solid #ccc;
    border-radius: 5px;
    transition: all 0.5s ease;
}

.wrapper .top_ a:hover {
    color: #333;
    border: 1px solid #333;
}

.wrapper .box {
    height:600px;
    width: 100%;
    border-left: 2px solid #EBEBEE;
    border-right: 2px solid #EBEBEE;
    border-top: 2px solid rgba(0, 0, 0, 0);
    border-bottom: 2px solid #EBEBEE;
    position: relative;
}

.wrapper .box .img_ {
    height: 140px;
    width: 120px;
    border-radius: 50%;
    overflow: hidden;
    position: absolute;
    top: -60px;
    left: 50%;
    margin-left: -65px;
}

.wrapper .box::before {
    content: "";
    display: block;
    position: absolute;
    height: 2px;
    width: 350px;
    background: #EBEBEE;
    left: 0px;
    top: 0px;
}

.wrapper .box::after {
    content: "";
    display: block;
    position: absolute;
    height: 2px;
    width: 350px;
    background: #EBEBEE;
    right: 0px;
    top: 0px;
}

.wrapper .box .img_ img {
    height: 100%;
    width: 100%;
}

.wrapper .box .img_ input {
    cursor: pointer;

    height: 100%;
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
    display: none;
    z-index: 10;
}

.wrapper .box .img_ label {
    height: 100%;
    width: 100%;
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    text-align: center;
    line-height: 140px;
    color: rgba(0, 0, 0, 0);
    background: rgba(0, 0, 0, 0);
    opacity: 1;
    cursor: pointer;
    font-weight: 200;
    z-index: 17；
}

.btn {
    margin-top: 10px;
}

.wrapper .box .img_ label:hover {
    color: #fff;
    background: rgba(0, 0, 0, 0.5);
}

.wrapper .box .img_ lable:hover {
    color: rgba(0, 0, 0, 0.0);
}

.wrapper .box .mess {
    width: 70%;
    height: 40px;
    margin: 100px auto 0px;
}

.wrapper .box .mess h2 {
    float: left;
    font-size: 22px;
    font-weight: 400;
    color: #333;
    margin:0;
}

.wrapper .box .mess a {
    float: right;
    text-decoration: none;
    border-radius: 5px;
    border: 1px solid #EBEBEE;
    display: block;
    padding: 6px 10px;
    color: #fff;
    background: #F93;
}

.wrapper .box .main {
    width: 80%;
    margin: 30px auto 0px;
}

.wrapper .box .main .lef {
    float: left;
    margin-left: 50px;
    text-align: right;
    color: #ccc;
}

.wrapper .box .main .lef,
.wrapper .box .main .rig {
    line-height: 30px;
    font-size: 16px;

}

.wrapper .box .main .rig {
    margin-left: 170px;
    color: #333;
}


.wrapper .box ul {
    width: 80%;
    margin: 0 auto;
}

.wrapper .box ul li {
    width: 100px;

    height: 40px;
    line-height: 40px;
    text-align: center;
    float: left;
}

.wrapper .box ul .li_ {
    margin-right: 75px;
}
