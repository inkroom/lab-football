.radio_btn{
    background: #fff;
    display: inline-block;
    width: 15px;
    height: 15px;
    border: 1px solid #66CCFF;
    border-radius: 15px;
    position: relative;
    vertical-align: middle;
    margin-top: -5px;
}
.radio_btn input{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100;
    opacity: 0;
}
.radio_btn span{
    background: #8fc0f3;
    width: 7px;
    height: 7px;
    display: inline-block;
    position: absolute;
    z-index: 1;
    top: 4px;
    left: 4px;
    border-radius: 10px;
}
.radio_btn input[type="radio"] + span{
    opacity: 0;
}
.radio_btn input[type="radio"]:checked + span{
    opacity: 1;
}
