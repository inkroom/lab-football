body{
    background-image: url("../../img/back1.png");
    margin: 0;
    padding: 0;
}
.header1{
    background-color: #434752;
    height: 50px;
}
.header2{
    background-color: #434752;
    height: 30px;
}
.container {
    padding-right: 0;
    padding-left: 0;
}
#c1 img{
    width: 180px;
}
#nav{
    background-color: #3185cb;
    width: 600px;
    height: 50px;
    margin:0 auto;
    z-index: 6;
    font-size: 18px;
    border-radius: 10px;
    position: absolute;
}
#nav ul{
    list-style: none;
    z-index: 6;
}
#nav ul li{
    float: left;
    line-height:50px;
    text-align: center;
    position: relative;
    width: 99px;
    border-right:1px solid #D0D0D0;
}
#nav a{
    text-decoration:none;
    color: #D0D0D0;
    display: block;
}
#nav a:hover{
    border-radius: 10px;
    color: #fff;
    background-color: #ff633c;
}
#nav ul li ul li{
    margin-top: 1px;
    float: none;
    background-color: #3185cb;
    border-radius: 10px;

}
#nav ul li ul{
    display: none;
    margin-top: 2px;
}
#nav ul li ul li a:hover{
    background-color: #f5a220;
}
#nav ul li:hover ul{
    display: block;
}
#nav-a{
    background-color: #3185cb;
    width: 280px;
    height: 30px;
    margin:0 auto;
    z-index: 2;
    font-size: 12px;
    border-radius:5px;
}
#nav-a ul{
    list-style: none;
}
#nav-a ul li{
    float: left;
    line-height:30px;
    text-align: center;
    position: relative;
    width: 55px;
}
#nav-a a{
    text-decoration:none;
    color: #D0D0D0;
    display: block;
}
#nav-a a:hover{
    border-radius:5px;
    color: #fff;
    background-color: #ff633c;
}
#nav-a ul li ul li{
    margin-top: 1px;
    float: none;
    background-color: #3185cb;
    border-radius: 5px;

}
#nav-a ul li ul{
    display: none;
    margin-top: 2px;
}
#nav-a ul li ul li a:hover{
    background-color: #f5a220;
}
#nav-a ul li:hover ul{
    display: block;
    top:30px;
    position: absolute;
}
