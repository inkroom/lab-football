* {
    margin: 0;
    padding: 0;
}

html {
    width: 100%;
    height: 100%;
    font-size: 100px
}

body, div, ul, ol, li, form, input, button, textarea, p {
    margin: 0;
    padding: 0;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
    font-size: inherit;
}

img {
    vertical-align: middle
}

a {
    text-decoration: none;
    cursor: pointer;
}

img {
    border: 0;
}

address, em, strong, th, var {
    font-style: normal;
    font-weight: normal;
}

caption, th {
    text-align: left;
}

textarea, select {
    font-family: inherit;
    font-size: inherit;
    font-weight: inherit;
}

li {
    list-style: none;
}

img, object {
    max-width: 100%;
}

input {
    border-width: 0px;
    text-align: center;
}

/* 导航栏2018/03/18*/
.nav-bg {
    background-color: #080615;
    font-size: 16px;
    height: 60px;
    margin: 0;
}

.logo-w {
    padding: 0;
    float: left;
}

.logo-w img {
    height: 60px;
    width: 60px;
}

.logo-w span {
    font-size: 18px;
    color: #fff;
}

.nav-b-1 button {
    padding: 0;
    margin: 10px 0;
    background-color: #3e3e59;
    color: #FFFFFF;
    width: 60px;
    height: 40px;
}

.nav-w ul li {
    text-align: center;
    float: left;
    width: 20%;
    line-height: 60px;
    list-style: none;
}

.nav-w ul li a {
    color: #999;
}

.nav-w ul li:hover {
    border-left: #e2374d 2px solid;
    color: #fff;
    background-color: #3e3e59;
    text-align: center;
    transform: skew(-30deg);
    -webkit-transform: skew(-30deg);
    -moz-transform: skew(-30deg);
    -o-transform: skew(-30deg);
    -ms-transform: skew(-30deg);
}

.nav-w ul li:hover a {
    font-size: 16px;
    color: #fff;
    text-decoration: none;
}

/*bottom*/

.bottom_box {
    width: 100%;
    height: 73px;
    position: fixed;
    bottom: 0px;
    border-top: 1px solid #E7E7E7;
    display: flex;
    background: #FFFFFF;
}

.tab_bar {
    flex: 1;
    text-align: center;
}

