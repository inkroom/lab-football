package cn.nsu.edu.web.four.dto.stc.referee;

import cn.nsu.edu.web.four.beans.schedule.Schedule;
import cn.nsu.edu.web.four.beans.teams.Team;

import java.util.List;

public class ScheduleInformationDto {
    private Schedule scheduleInformation;
    private Team teamA;
    private Team teamB;

    public Schedule getScheduleInformation() {
        return scheduleInformation;
    }

    public void setScheduleInformation(Schedule scheduleInformation) {
        this.scheduleInformation = scheduleInformation;
    }

    public Team getTeamA() {
        return teamA;
    }

    public void setTeamA(Team teamA) {
        this.teamA = teamA;
    }

    public Team getTeamB() {
        return teamB;
    }

    public void setTeamB(Team teamB) {
        this.teamB = teamB;
    }

    @Override
    public String toString() {
        return "ScheduleInformationDto{" +
                "scheduleInformation=" + scheduleInformation +
                ", teamA=" + teamA +
                ", teamB=" + teamB +
                '}';
    }
}
