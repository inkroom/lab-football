/*详情幻灯片
 2018/3/08*/
.pic {
    width: 700px;
    height: 300px;
    position: relative;
    overflow: hidden;
    float: left;
    margin-left: 5px;
    margin-bottom: 10px;
    border: 1px solid #FFFFFF;
    box-shadow: 0 0px 24px 0 rgba(0, 0, 0, 0.06), 0 1px 0px 0 rgba(0, 0, 0, 0.02);
}

.tit {
    height: 15px;
    position: relative;
    top: -25px;
}

.tit a {
    /*padding:0 30px;*/
    width: 15px;
    border-radius: 15px;
    height: 100%;
    float: left;
    text-align: center;
    line-height: 15px;
    font-size: 5px;
    color: #d3a268;
    text-decoration: none;
    position: relative;
    left: 45%;
    z-index: 1;
}

.tit .float {
    width: 15px;
    border-radius: 15px;
    height: 100%;
    background: #d3a268;
    position: absolute;
    left: 45%;
}

.box-1 {
    width: 100%;
    height: 300px;
    position: relative;
}

.box-1 div {
    width: 100%;
    height: 100%;
    left: 100%;
    position: absolute;
    font-size: 16px;
    top: 0;
}

.rightB {
    width: 50px;
    height: 70px;
    font-size: 30px;
    color: #fff;
    position: absolute;
    right: 0;
    top: 40%;
    line-height: 70px;
    text-align: center;
    cursor: pointer;
    display: none;
}

.leftB {
    width: 50px;
    height: 70px;
    font-size: 30px;
    color: #fff;
    position: absolute;
    left: 0;
    top: 40%;
    line-height: 70px;
    text-align: center;
    cursor: pointer;
    display: none;
}
