/*球队管理*/
body{
  font-size:14px;
  font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
}


/*nav*/
.nav-bg{
  background-color: rgba(199,217,243,1);
  height: 60px;
  box-sizing: border-box;
  padding:1px;
  /*margin:0;*/
}
.logo-w {
  padding:0;
  float: left;
}

.logo-w img {
  height: 60px;
  width: 60px;
}

.logo-w span a{
  color:#669;
  text-decoration: none;
  font-size: 17px;
}
.nav-w{
  margin-left:-1px;
}
#nav-list{
  width:843px;
  position: relative;
  clear:both;
}
#nav-list>li {
  text-align: center;
  float: left;
  width: 80px;
  margin-left:40px;
  line-height: 60px;
  list-style: none;
  cursor: pointer;

}
#nav-list>li a{
  color:#669;
  text-decoration: none;
}
#nav-list>li:hover a{
  color: #339;
}
.move{
  height: 4px;
  width: 60px;
  border-top: 4px solid #339;/*只需要将上边显示出来*/
  position: absolute;
  left: 360px;
  top: 0;
  transition: left .3s ease-out 0s;/*包含四个过度属性：执行变换属性、执行时间、速率、延迟*/
  -webkit-transition: left .3s ease-out 0s;/*chrome和safari*/
  -moz-transition: left .3s ease-out 0s;/*firefox*/
  -o-transition: left .3s ease-out 0s;/*opera*/
}

#nav-list>li:nth-child(1):hover~ .move{    /*li元素的父元素的第一个子元素，当鼠标停留其上时，move元素的left属性改变，活动区间120px*/
  left: 0;
}
#nav-list>li:nth-child(2):hover~ .move{
  left: 120px;
}
#nav-list>li:nth-child(3):hover~ .move{
  left: 240px;
}
#nav-list>li:nth-child(4):hover~ .move{
  left: 360px;
}

#user2{
  clear:left;
  list-style: none;
  width:350px;
  position: absolute;
  top:22px;
  left:40px;
  display: none;
}
#user2 li{
 /* margin:3px 10px 0px -20px;*/
 /* padding-left:15px;*/
  padding:0 0 10px 0;
  width:80px;
  float:left;
  display:block;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}

/*add*/
#userOperator{
  display:none;
  position:relative;
  top:-1px;
  left:-25px;
  list-style: none;
  height:60px;
  width:80px;
  background-color: rgba(199,217,243,1);
}
#userOperator li{
  z-index:2;
  line-height:30px;
}
/*nav   end*/
