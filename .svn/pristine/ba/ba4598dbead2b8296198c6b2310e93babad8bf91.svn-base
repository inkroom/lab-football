/*导航栏*/
.nav-bg{
	background-color: #080615;
	height: 60px;
	margin:0;
}
.nav-b{
	font-size: 32px;
	line-height: 60px;
}
.logo-w{
	padding: 0;
	float: left;
	/*	background-color: #2AABD2;*/
}
.logo-w img{
	height: 60px;
	width: 60px;
}
.logo-w span{
	font-size: 18px;
	color: #fff;
}

.logo-w2{
	padding: 0;
	float: left;
}

.logo-w2 img{
	height: 35px;
	width: 35px;
}
.logo-w2 span{
	font-size: 18px;
	color: #0f0f0f;
}
.logo-w span a{
	color: #fff;
	text-decoration: none;
}
.nav-w ul li{
	text-align: center;
	float: left;
	width: 20%;
	line-height: 60px;
	list-style: none;
}
.nav-w ul li a{
	color: #999;
}
.nav-w ul li:hover{
	border-left: #e2374d 2px solid;
	color: #fff;
	background-color: #3e3e59;
	text-align: center;
	transform:skew(-30deg);
	-webkit-transform: skew(-30deg);
	-moz-transform: skew(-30deg);
	-o-transform:skew(-30deg);
	-ms-transform:skew(-30deg);
}
.nav-w ul li:hover a{
	font-size: 16px;
	color: #fff;
	text-decoration: none;
}
.head{
	height: 60px;
}

.btn{
	width: 200px;
	height: 100px;
	font-size: 40px;
}

.container{
	display:table;
	height:100%;
}

.row{
	display: table-cell;
	vertical-align: middle;
}

.login_bg{
	background: rgba(255,255,255,0.6);
	border-radius: 8px;
	height: 80%;
}

.login_bg span{
	color: white;
}


.input{
	width: 50px;
}

