          <input type="text" class="form-control" style="width:142px;border-radius: 5px;" placeholder="请输入球员班级">
        </div>
        <!-- /input-group -->
      </div>
      <div class="span2">
        <div class="input-group">
          <input type="text" class="form-control" style="width:142px;border-radius: 5px;" placeholder="请输入球员姓名">
