html, body, #box {
	height:100%;
}
body {
	text-align:center;
}
#box {
	text-align:left;
	display:table;
	width:100%;
	margin:0 auto;
	position:relative;
}
#box > div {
	display:table-row;
}
#header{
	height:50px;
	vertical-align:bottom;
}
#middle{
	height:70px;
	vertical-align:bottom;
}
#footer {
	height:10px;
	vertical-align:bottom;
}
#main #wrap {
	margin-bottom: 200px;
	display:table-cell;
	vertical-align:middle;
}
#text {
	text-align:center;
}
.body-height{
	overflow-y:scroll;
	height: 100%;
}
.bor-bottom{
	border-bottom:#eee 1px solid;
}
.font-size1{
	font-size: 16px;
}
.font-size2{
	font-size: 14px;
}
.font-size3{
	font-size: 13px;
}
.font-size4{
	font-size: 20px;
}
.nav-style{
	height:55px;
	margin: 0;
	position: relative;
}
.div-place{
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
}
.img-style{
	height: 20px;
	width: 20px;
}
.color1{
	color: #333;
}
.float{
	float: left;
}
.margin{
	margin-top: 5px;
}
.img-style1{
	width: 30px;
	height: 30px;
	float: right;
}
.div-height{
	height: 120px;
}
.font-weight{
	font-weight: 600;
}
.div-style1{
	width: 100%;
	height: 40px;
	margin-top: 10px;
}
.div-style2{
	margin-left: 5%;
}
.img-style2{
	width: 50px;
	margin: 0 20px;
}
.img-style3{
	width: 20px;
}
.div-style3{
	height:50px;
}
.div-style4{
	width: 80px;
	height: auto;
	max-height:100px;
}
.div-style5{
	max-width: 800px;
	max-height: 100px;
}
.width{
	width: 30%;
}
.height{
	height: 30px;
}
.width1{
	width: 40px;
}
.padding-top{
	padding-top: 5px;
}
.float-right{
	float: right;
}
.hr-style{
	margin: 10px 0;
}
.none-padding{
	padding: 0 0;
}
/*模态框*/
textarea::-webkit-input-placeholder{
	color:#ccc;
}
textarea:-moz-placeholder{
	color:#ccc;
}
textarea::-moz-placeholder{
	color:#ccc;
}
textarea:-ms-input-placeholder{
	color:#ccc;
}
.bor{
	border: #eee 1px solid;
}
.text-style{
	width: 100%;
	resize: none;
	outline: none;
}
.input-style{
	width: 80px;
	height: 80px;
	position: relative;
	cursor: pointer;
	outline: medium none;
	opacity:0;
	margin-top: -80px;
