<!-- 模态框3Modal 开始-->
<div id="myModal3" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 id="myModalLabel">球队信息</h3>
  </div>
  <div class="modal-body">
    <div class="span6">
      <table class="table table-bordered " id="messageCard">
        <tr class="success">
          <td>基本信息</td>
        </tr>
        <tr>
          <td>
            <p><span class="muted">队&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;名:</span>
              <input type="text" style="height:20px;width:190px;"></p>
            <p><span class="muted">球队类型:</span>
              <select id="myInput" style="width:204px;">
                <option value="小学一年级">小学一年级</option>
                <option value="小学二年级">小学二年级</option>
                <option value="小学三年级">小学三年级</option>
                <option value="小学四年级">小学四年级</option>
                <option value="小学五年级">小学五年级</option>
                <option value="小学六年级">小学六年级</option>
                <option value="初中一年级">初中一年级</option>
                <option value="初中二年级">小学二年级</option>
                <option value="初中三年级">小学三年级</option>
                <option value="混合(年级)">混合(年级)</option>
                <option value="混合(年级)">混合(校级)</option>
                <select></p>
            <p></p><span class="muted">球队状态:</span>
            <input type="radio" name="TEAM_STATUS" checked="checked" value="在役"> 在役
            <input type="radio" name="TEAM_STATUS" value="退役"> 退役
            </p>
            <p><span class="muted">球队编号: </span><span >123456789</span></p>
            <p><span class="muted">创建时间: </span><span >2018-3-21</span></p>
          </td>
        </tr>
      </table>
    </div>

    <div class="span6">
      <table class="table table-bordered table-hover messageCard">
        <tr class="success">
          <td>教练信息</td>
        </tr>
        <tr>
          <td>
            <p style="margin-top:6px;"><span class="muted">姓&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;名: <span></span></span></p>
            <p style="margin-top:32px;"><span class="muted">教练编号:</span></p>

          </td>
        </tr>
      </table>
    </div>


    <div class="span6">
      <table class="table table-bordered " id="messageCard">
        <tr class="success">
          <td>球员信息</td>
        </tr>
        <tr>
          <td>
            <div class="row">

              <div class="zhazhazha">
                <img class="clo" src="../../../resources/img/22.png" style="width:100%;">
                <p class="muted zhazhahui" >
                  <span>渣渣渣</span><br>
                  <span>123456</span>
                </p>
