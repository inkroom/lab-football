<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!--底部-->
<div class="bottom_box" >
    <div class="tab_bar" id="index" onclick="document.location.href='${path}/referee/index';">
        <img src="${path}/resources/img/index${pageContext.request.getRequestURI().contains('index')?'-on':''}.png" />
        <p>后台首页</p>
    </div>
    <div class="tab_bar ${pageContext.request.getRequestURI().contains('test')?'choose':''}" id="identity" onclick="document.location.href='${path}/referee/check';">
        <img src="${path}/resources/img/identity${pageContext.request.getRequestURI().contains('score')?'-on':''}.png"/>
        <p>入场验证</p>
    </div>
    <div class="tab_bar ${pageContext.request.getRequestURI().contains('qrCode')?'choose':''}" id="QRcode" onclick="document.location.href='${path}/referee/QRcode';">
        <img src="${path}/resources/img/QRcode${pageContext.request.getRequestURI().contains('score')?'-on':''}.png"/>
        <p>二维码</p>
    </div>
    <div class="tab_bar ${pageContext.request.getRequestURI().contains('score')?'choose':''}" id="revisel" onclick="document.location.href='${path}/referee/modify';">
        <img src="${path}/resources/img/revise1${pageContext.request.getRequestURI().contains('score')?'-on':''}.png"/>
        <p  style="color: #5496F9;">修改比分</p>
    </div>
</div>
<input type="hidden" value="${token}" id="token">
