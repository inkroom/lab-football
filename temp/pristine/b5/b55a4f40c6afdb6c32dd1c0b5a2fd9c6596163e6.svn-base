<script src="${path}/resources/js/bootstrap.min.js"></script>
<!--<script src="layer_mobile/layer.js"></script>-->
<script>





    $('.game-add').on('click',function(){
        $('#add-page').addClass('show-hide');
        $('#select-team').attr({'disabled':'disabled'});
        $('#time').attr({'disabled':'disabled'});
        $('#reason').attr({'disabled':'disabled'});
        $('#confim').on('click',function(){
            var $tr = $("<tr></tr>");  //创建tr
            var $btn = $("<td><input type='button' class='btn btn-danger btn-xs center-block' id='del' value='删除'/></td>");
            var $name = '<td>'+$('#select-name option:selected').text()+'</td>'; //创建td
            var $place = '<td>'+$('#select-place option:selected').text();+'</td>';
            var $num = '<td>'+$("#shirt-num").val()+'</td>';
            $tr.append($name,$place,$num,$btn); //将获取的值追加到tr中
            $('#tab').append($tr); //将获取的tr追加到 table中
            $('#del').on('click',function(){
                var p1 = this.parentNode;
                var p2 = p1.parentNode;
                $(p2).remove();
            });
//					$('#select-name').get(0).selectedIndex=0;
//					$('#select-place').get(0).selectedIndex=0;
//					$('#shirt-num').val('');
            $('#add-page').removeClass('show-hide');
        });
        $('#cancel').on('click',function(){
            $('#select-name').get(0).selectedIndex=0;
            $('#select-place').get(0).selectedIndex=0;
            $('#shirt-num').val('');
            $('#add-page').removeClass('show-hide');
        });
    });
</script>
